import math
'''
class Node:
    def __init__(self, value):
        self.value = value
        self.next = None
'''

def odd_divisors_ll(N):
    head=None
    tail=None
    for i in range(1,N+1):
        if math.isqrt(i)**2==i:
            new_node=Node(i)
            if head is None:
                head=new_node
                tail=new_node
            else:
                tail.next=new_node
                tail=new_node
    return head
            
